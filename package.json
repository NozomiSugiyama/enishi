{
    "name": "enishi",
    "version": "0.0.1",
    "main": "src/index.tsx",
    "repository": "https://github.com/NozomiSugiyama/enishi",
    "author": "Nozomi Sugiyama <rioc.sugiyama@gmail.com>",
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": "20.8.x"
    },
    "scripts": {
        "dev": "env NODE_ENV=development webpack serve --config webpack.config.js",
        "test": "jest",
        "build": "yarn build:prod",
        "build:dev": "env NODE_ENV=development webpack --mode=development --config webpack.config.js",
        "build:stg": "env NODE_ENV=production webpack --mode=production --config webpack.config.js",
        "build:prod": "env NODE_ENV=production webpack --mode=production --config webpack.config.js",
        "lint": "eslint './src/**/*.ts{,x}'",
        "lint:report": "eslint --output-file eslint_report.json --format json './src/**/*.ts{,x}'",
        "format": "prettier -c .",
        "format:fix": "prettier -w .",
        "depcruise": "depcruise --config .dependency-cruiser.js .",
        "size-analyze": "yarn build:prod --json | tail -n +2 | webpack-bundle-size-analyzer",
        "coverage:ci": "yarn test --coverage --silent --ci --testLocationInResults --json --outputFile=\"report.json\""
    },
    "dependencies": {
        "@emoji-mart/react": "^1.1.1",
        "@react-icons/all-files": "https://github.com/react-icons/react-icons/releases/download/v4.11.0/react-icons-all-files-4.11.0.tgz",
